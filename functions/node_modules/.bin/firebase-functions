#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/lib/bin/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/lib/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/lib/bin/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/lib/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules/firebase-functions/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/firebase-functions@6.2.0_firebase-admin@11.11.1/node_modules:/Users/derickfiebiger/app-beratrack/functions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
else
  exec node  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
fi
